{
  "build": {
    "beforeDevCommand": "yarn workspace @test-rust/app-desktop run dev:vue",
    "beforeBuildCommand": "yarn workspace @test-rust/app-desktop run build:vue",
    "devPath": "http://localhost:1420/",
    "distDir": "../dist"
  },
  "package": {
    "productName": "test-rust-desktop",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
      "identifier": "com.ukonnra.wonderland.whiterabbit",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "maximized": true,
        "resizable": true,
        "title": "Wonderland WhiteRabbit Desktop"
      }
    ]
  }
}
